# Created by Chef for tyalexander

UseKeychain yes
AddKeysToAgent yes

Host 127.0.0.1
  ForwardAgent yes
  ServerAliveInterval 60

Host localhost
  ForwardAgent yes
  ServerAliveInterval 60

Host *.local
  ServerAliveInterval 60
  ForwardAgent yes
  User tyalexander

Host jump
  ServerAliveInterval 60
  ForwardAgent yes
  User tyalexander
  StrictHostKeyChecking no

Host *.sendgrid.net
  ServerAliveInterval 60
  ForwardAgent yes
  User tyalexander
  StrictHostKeyChecking no



