#! /usr/bin/env python

import os
import string
import shutil

for filename in os.listdir("."):
    letter = string.lower(filename[:1])
    if filename == ".AppleDouble" :
        continue
    if filename == ".DS_Store" :
        continue
    os.path.exists(letter) or os.mkdir(letter)
    #print "mv '%s' %s" % (filename, letter)
    shutil.move(filename, letter)
